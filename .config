CONFIG_CPU=esp8266
CONFIG_BOARD=nodemcu
CONFIG_ESPBAUD=230400
# CONFIG_ESPPORT is not set

CONFIG_FLASH_MODE=dio
CONFIG_FLASH_FREQ=40
CONFIG_FLASH_SIZE=4MB

CONFIG_RF_INIT_DATA=esp_init_data_default.bin

CONFIG_IRAM_ADDR=0x00000
CONFIG_IROM_ADDR=0x40000

CONFIG_OTA=n
# CONFIG_FLASH_MAP_EXTEND=y
CONFIG_OTA_BOOTLOADER=boot_v1.5
